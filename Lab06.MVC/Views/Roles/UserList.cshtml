@model IEnumerable<Lab06.MVC.DAL.Entities.StudentEntity>

<div class="container mt-5">
    <h2>List of users</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Email</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Email</td>
                    <td>
                        <a class="btn btn-outline-primary" asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
